{
  "name": "auto-prettier-eslint",
  "main": "./dist/auto-prettier-eslint.js",
  "version": "2.0.2",
  "description": "Runs eslint --fix or prettier on the current file.",
  "keywords": [
    "prettier",
    "eslint",
    "format"
  ],
  "repository": "",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "10.0.0",
    "atom-package-deps": "5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2"
  },
  "apmInstallSource": {
    "type": "git",
    "source": "https://gitlab.com/serk12/eslint-fixer-pulsar",
    "sha": "e041d257b389f82ac6e6f5480e4893e654f83cdb"
  },
  "package-deps": [
    "linter:2.0.0"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "scripts": {
    "build": "clear ; rm -rf dist ;  npx tsc -p tsconfig.json || echo done"
  },
  "devDependencies": {
    "@types/atom": "^1.40.18",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
