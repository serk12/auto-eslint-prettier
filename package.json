{
  "name": "auto-eslint-prettier",
  "main": "./dist/auto-eslint-prettier.js",
  "version": "2.0.13",
  "description": "Runs eslint --fix or prettier on the current file.",
  "keywords": [
    "prettier",
    "eslint",
    "format"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/serk12/auto-eslint-prettier"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "10.0.0",
    "atom-package-deps": "5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2"
  },
  "package-deps": [
    "linter:2.0.0"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "scripts": {
    "build": "clear ; rm -r dist ;  npx tsc -p tsconfig.json || echo done"
  },
  "devDependencies": {
    "@types/atom": "^1.40.18",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^7.2.1",
    "typescript": "^5.3.3"
  }
}
